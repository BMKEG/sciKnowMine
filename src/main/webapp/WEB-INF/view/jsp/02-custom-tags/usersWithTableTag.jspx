<div xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" 	xmlns:custom="urn:jsptagdir:/WEB-INF/tags">	<jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />	<jsp:directive.page contentType="text/html;charset=UTF-8" /> 	<custom:mainLayout title="${title}/">			<custom:simpleTable collection="${users}" headerLabels="First Name, Last Name">			<c:forEach var="user" items="${users}">				<tr>					<td> <c:out value="${user.firstName}"/> </td>					<td> <c:out value="${user.lastName}"/> </td>				</tr>			</c:forEach>		</custom:simpleTable>	</custom:mainLayout></div>